{ "extends"       : "eslint:recommended",
  "env"           : { "browser" : true, "es6" : true },
  "parserOptions" : { "ecmaVersion": 2018 },
  "rules"         :
    { "strict"           : [ "error", "safe" ],
      "no-extra-semi"    : "off",
      "no-console"       : "off",
      "no-extend-native" : [ "error" ],
      "no-unused-vars"   : [ "error", { "vars" : "local" } ] } }
