{ "extends"              : "eslint:recommended",
  "env"                  : { "browser" : true, "es2020" : true },
  "rules"                :
    { "strict"           : [ "error", "safe" ],
      "no-extra-semi"    : "off",
      "no-console"       : "off",
      "no-extend-native" : [ "error" ],
      "no-unused-vars"   : [ "error", { "vars" : "local" } ] } }
